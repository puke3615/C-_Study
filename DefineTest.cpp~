	cout << "行号 -- LINE --> " << __LINE__ << endl;
	cout << "文件 -- FILE --> " << __FILE__ << endl;
	cout << "日期 -- DATE --> " << __DATE__ << endl;
	cout << "事件 -- TIME --> " << __TIME__ << endl;
	
